{
  "name": "job-notifier-manager",
  "version": "1.0.0",
  "description": "Job Notifier App with integrated Express backend",
  "main": "expo-router/entry",
  "esbuild": {
    "external": [
      "react-native"
    ]
  },
  "scripts": {
    "lint": "expo lint",
    "reset-project": "node ./scripts/reset-project.js",
    "test": "jest",
    "backend": "cd server && nodemon --exec tsx index.ts",
    "frontend:local": "npx expo start",
    "typescript": "npx tsc -noEmit --watch",
    "start": "cd server && npm start",
    "frontend": "npx expo start --tunnel",
    "dev": "concurrently \"cd server && npm run dev\" \"npx expo start --tunnel -c\"",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "server": "cd server && npm start",
    "dev-server": "cd server && npm run dev",
    "install-all": "npm install && cd server && npm install"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.6.2",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "@sentry/react-native": "6.20.0",
    "@supabase/supabase-js": "^2.57.4",
    "body-parser": "^1.20.3",
    "expo": "~54.0.9",
    "expo-application": "~7.0.7",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.12",
    "expo-device": "~8.0.8",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "express": "^5.1.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "nativewind": "^4.1.23",
    "node-fetch": "^3.3.2",
    "playwright": "^1.55.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "^0.81.4",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-reanimated-carousel": "^4.0.2",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "sentry-expo": "~7.0.0",
    "tailwind": "^2.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@sentry/cli": "^2.53.0",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/next": "^8.0.7",
    "@types/node": "^24.3.1",
    "@types/react": "~19.1.10",
    "concurrently": "^9.2.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "jsdom": "^26.1.0",
    "next": "^15.5.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "~5.9.2"
  },
  "keywords": [
    "job-tracker",
    "puppeteer",
    "web-scraping",
    "express",
    "react-native",
    "expo"
  ],
  "license": "MIT",
  "private": true
}
