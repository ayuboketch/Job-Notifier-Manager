{
  "name": "job-notifier-manager",
  "version": "1.0.0",
  "description": "Job Notifier App with integrated Express backend",
  "main": "expo-router/entry",
  "esbuild": {
    "external": [
      "react-native"
    ]
  },
  "scripts": {
    "lint": "expo lint",
    "reset-project": "node ./scripts/reset-project.js",
    "test": "jest",
    "backend": "cd server && nodemon --exec tsx index.ts",
    "frontend": "npx expo start -c --tunnel",
    "frontend:local": "npx expo start",
    "typescript": "npx tsc -noEmit --watch",
    "start": "npx expo start --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "server": "cd server && npm start",
    "dev-server": "cd server && npm run dev",
    "dev": "concurrently \"cd server && npm run dev\" \"npx expo start --tunnel -c\"",
    "install-all": "npm install && cd server && npm install"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^7.4.5",
    "@react-navigation/elements": "^2.6.2",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "@supabase/supabase-js": "^2.57.4",
    "expo": "^53.0.22",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.6",
    "expo-font": "~13.3.1",
    "expo-haptics": "14.1.4",
    "expo-image": "~2.4.0",
    "expo-linear-gradient": "14.1.5",
    "expo-linking": "~7.1.5",
    "expo-router": "^5.1.6",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "^5.0.11",
    "expo-web-browser": "14.2.0",
    "jest": "^29.7.0",
    "nativewind": "^4.1.23",
    "node-fetch": "^3.3.2",
    "playwright": "^1.55.0",
    "playwright-core": "^1.55.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "^0.79.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-reanimated-carousel": "^4.0.2",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "tailwind": "^2.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/next": "^8.0.7",
    "@types/node": "^24.3.1",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.72.8",
    "concurrently": "^9.2.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "jsdom": "^26.1.0",
    "next": "^15.5.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3"
  },
  "keywords": [
    "job-tracker",
    "puppeteer",
    "web-scraping",
    "express",
    "react-native",
    "expo"
  ],
  "license": "MIT",
  "private": true
}
