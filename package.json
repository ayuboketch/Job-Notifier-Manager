{
  "name": "job-notifier-manager",
  "version": "1.0.0",
  "description": "Job Notifier App with integrated Express backend",
  "main": "expo-router/entry",
  "scripts": {
    "start": "npx expo start --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "reset-project": "node ./scripts/reset-project.js",
    "server": "tsx server/index.ts",
    "test": "jest",
    "dev-server": "nodemon --watch server --exec tsx server/index.ts",
    "dev": "concurrently --raw \"cd server && nodemon --exec tsx index.ts\" \"npx expo start --tunnel\"",
    "backend": "cd server && nodemon --exec tsx index.ts",
    "frontend": "npx expo start --tunnel",
    "frontend:local": "npx expo start"
  },
